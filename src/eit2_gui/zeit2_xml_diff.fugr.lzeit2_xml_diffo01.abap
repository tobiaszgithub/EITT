MODULE pbo_100 OUTPUT.
  SET PF-STATUS 'STATUS_100'.
*  SET TITLEBAR 'xxx'.
  go_xml_diff = NEW zcl_eit2_xml_diff( iv_pri = gv_pri
                                          iv_sec = gv_sec ).
  go_xml_diff->show_dialog_screen( ).

ENDMODULE.
*&---------------------------------------------------------------------*
*&      Module  PAI_100  INPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE pai_100 INPUT.
  CASE sy-ucomm.
    WHEN 'ENTER' OR 'CANCEL'.
      go_xml_diff->free( ).
      LEAVE TO SCREEN 0.
    WHEN OTHERS.
  ENDCASE.
ENDMODULE.
